{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "plugins": [
        "unicorn",
        "sonarjs"
    ],
    "extends": ["eslint:recommended", "plugin:unicorn/recommended", "plugin:sonarjs/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "unicorn/no-null": "off",
        "sonarjs/prefer-while": "off"
    }
}
